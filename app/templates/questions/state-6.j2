{% extends "questions/base.j2" %}

{% block content %}
<li>
  <label for="credits_taken">Credits Taken:</label>
  <input
    placeholder="credits taken"
    type="number"
    min="0"
    max="150"
    step="1"
    pattern="\d+"
    id="credits_taken""
    name="credits_taken"
    value="{{ credits_taken }}"
  >
</li>
{% endblock %}
